{
  "name": "@techmely/vike-react-query",
  "version": "3.0.2",
  "homepage": "https://techmely.com/libraries/vike-react",
  "bugs": {
    "url": "https://github.com/techmely/vike-extension/issues"
  },
  "repository": {
    "url": "git+https://github.com/techmely/vike-extension.git#main"
  },
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint.format": "bunx @biomejs/biome check --write .",
    "clean": "rm -f dist node_modules",
    "test.unit": "vitest --passWithNoTests",
    "test.unit.run": "NODE_ENV=test vitest run --passWithNoTests --isolate",
    "test.unit.coverage": "vitest --coverage",
    "build": "cross-env NODE_ENV=production bun run ./build.ts",
    "release": "bun run ./release.ts",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.49.2",
    "@techmely/utils": "^3.3.0",
    "devalue": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-streaming": "^0.3.35",
    "vike": "^0.4.177"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.7",
    "@techmely/types": "1.9.0",
    "@testing-library/react": "16.0.0",
    "@types/bun": "1.1.6",
    "@types/node": "20.14.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "1.6.0",
    "cross-env": "7.0.3",
    "happy-dom": "14.12.3",
    "husky": "9.0.11",
    "npm-check-updates": "16.14.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-streaming": "0.3.28",
    "typescript": "5.5.3",
    "vike": "^0.4.177",
    "vite": "^5.3.3",
    "vitest": "2.0.2",
    "zx": "8.1.4"
  },
  "exports": {
    ".": "./index.js",
    "./config": "./+config.js",
    "./query/config": "./query/+config.js",
    "./useData": "./hooks/useData.js",
    "./usePageContext": "./hooks/usePageContext.js",
    "./clientOnly": "./helpers/clientOnly.js",
    "./render/onRenderHtml": "./render/onRenderHtml.js",
    "./render/onRenderClient": "./render/onRenderClient.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./index.d.ts"
      ],
      "clientOnly": [
        "./components/clientOnly.d.ts"
      ],
      "FallbackErrorBoundary": [
        "./components/FallbackErrorBoundary.d.ts"
      ],
      "withFallback": [
        "./components/withFallback.d.ts"
      ],
      "useData": [
        "./hooks/useData.d.ts"
      ],
      "usePageContext": [
        "./hooks/usePageContext.d.ts"
      ],
      "ReactQueryProvider": [
        "./providers/ReactQueryProvider.d.ts"
      ],
      "query/config": [
        "./query/+config.d.ts"
      ],
      "config": [
        "./+config.d.ts"
      ],
      "renderer/onRenderHtml": [
        "./renderer/onRenderHtml.d.ts"
      ],
      "renderer/onRenderClient": [
        "./renderer/onRenderClient.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "tag": "latest"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/techmely"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/techmely"
    }
