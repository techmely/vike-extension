{
  "name": "@techmely/vike-react",
  "version": "2.2.0",
  "homepage": "https://about.techmely.com/libraries/vike-react",
  "bugs": {
    "url": "https://github.com/techmely/essential-packages/issues"
  },
  "repository": {
    "url": "git+https://github.com/techmely/essential-packages.git#main",
    "directory": "ts-package/vike-react"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "lint.format": "bunx @biomejs/biome check --write .",
    "clean": "moon :clean",
    "test.unit": "vitest --passWithNoTests",
    "test.unit.run": "NODE_ENV=test vitest run --passWithNoTests --isolate",
    "test.unit.coverage": "vitest --coverage",
    "build": "bun build ./src/index.ts --target bun --outdir dist --sourcemap=linked && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "prepare": "husky"
  },
  "dependencies": {
    "@techmely/head": "^0.0.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.49.2",
    "@techmely/utils": "^3.3.0",
    "devalue": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-streaming": "^0.3.35",
    "vike": "^0.4.177"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.7",
    "@testing-library/react": "16.0.0",
    "@types/bun": "1.1.6",
    "@types/node": "20.14.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "1.6.0",
    "husky": "9.0.11",
    "npm-check-updates": "16.14.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-streaming": "0.3.28",
    "tsup": "8.1.0",
    "typescript": "5.5.3",
    "vike": "^0.4.177",
    "vite": "^5.3.3",
    "vitest": "2.0.2"
  },
  "exports": {
    ".": "./dist/index.js",
    "./config": "./dist/+config.js",
    "./query/config": "./dist/query/+config.js",
    "./useData": "./dist/hooks/useData.js",
    "./usePageContext": "./dist/hooks/usePageContext.js",
    "./clientOnly": "./dist/helpers/clientOnly.js",
    "./render/onRenderHtml": "./dist/render/onRenderHtml.js",
    "./render/onRenderClient": "./dist/render/onRenderClient.js"
  },
  "typesVersions": {
    "*": {
      "query/config": [
        "./dist/query/_config.d.ts"
      ],
      "useData": [
        "./dist/hooks/useData.d.ts"
      ],
      "usePageContext": [
        "./dist/hooks/usePageContext.d.ts"
      ],
      "clientOnly": [
        "./dist/helpers/clientOnly.d.ts"
      ],
      ".": [
        "./dist/index.d.ts"
      ],
      "config": [
        "./dist/+config.d.ts"
      ],
      "renderer/onRenderHtml": [
        "./dist/renderer/onRenderHtml.d.ts"
      ],
      "renderer/onRenderClient": [
        "./dist/renderer/onRenderClient.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "tag": "latest"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/techmely"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/techmely"
    }
  ],
  "keywords": [
    "techmely",
    "techmely-react",
    "vike react"
  ]
}
